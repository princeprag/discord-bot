{
  "name": "discord_bot",
  "author": "Nicholas Carrigan",
  "main": "./prod/src/main.js",
  "version": "4.22.0",
  "license": "GPL-3.0",
  "private": false,
  "engines": {
    "node": "14.2.0"
  },
  "keywords": [
    "DiscordJS",
    "Discord",
    "bot",
    "discord-bot"
  ],
  "repository": "https://github.com/nhcarrigan/discord-bot",
  "bugs": {
    "url": "https://github.com/nhcarrigan/discord-bot/issues"
  },
  "homepage": "https://github.com/nhcarrigan/discord-bot#readme",
  "scripts": {
    "build": "rm -rf ./prod && tsc",
    "dev": "nodemon",
    "dev:command": "npm run build && npm run start:ts",
    "lint": "eslint src --fix",
    "start": "node ./prod/src/main.js",
    "start:ts": "ts-node -r ./src/main.ts",
    "pretest:unit": "npm run build",
    "test:unit": "mocha './test/unit/**/*spec.ts'"
  },
  "_moduleAliases": {
    "@Commands": "prod/src/commands",
    "@Database": "prod/src/database/main.js",
    "@Events": "prod/src/events",
    "@Interceptors": "prod/src/interceptors",
    "@Interfaces": "prod/src/interfaces",
    "@Listeners": "prod/src/listeners",
    "@Models": "prod/src/database/models",
    "@Utils": "prod/src/utils"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "discord.js": "^12.3.1",
    "mathjs": "^7.3.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.7"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/expect": "^24.3.0",
    "@types/mathjs": "^6.0.5",
    "@types/mocha": "^8.0.3",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.2",
    "@types/sinon": "^9.0.7",
    "@types/sinon-chai": "^3.2.5",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "ts-mock-imports": "^1.3.0",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx eslint src --fix"
    }
  }
}
