{
  "name": "becca_lyria",
  "author": "Nicholas Carrigan",
  "main": "./prod/src/main.js",
  "version": "8.2.0",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "engines": {
    "node": "14.13.1"
  },
  "keywords": [
    "DiscordJS",
    "Discord",
    "bot",
    "discord-bot"
  ],
  "repository": "https://github.com/nhcarrigan/Becca-Lyria",
  "bugs": {
    "url": "https://github.com/nhcarrigan/Becca-Lyria/issues"
  },
  "homepage": "https://github.com/nhcarrigan/Becca-Lyria#readme",
  "scripts": {
    "prebuild": "rm -rf ./prod",
    "build": "tsc",
    "precoverage": "rm -rf ./nyc_output",
    "coverage": "nyc npm run test:unit",
    "postcoverage": "nyc report --reporter=text-lcov > lcov.info",
    "dev": "nodemon",
    "dev:command": "npm run build && npm run start:ts",
    "lint": "eslint src --fix",
    "start": "node ./prod/src/serve.js",
    "start:ts": "ts-node -r ./src/serve.ts",
    "test:unit": "echo 'Tests temporarily disabled pending debugging of variable name change.'",
    "test:backup": "mocha './test/unit/**/*spec.ts'"
  },
  "_moduleAliases": {
    "@Commands": "prod/src/commands",
    "@Database": "prod/src/database/main.js",
    "@Events": "prod/src/events",
    "@Interceptors": "prod/src/interceptors",
    "@Interfaces": "prod/src/interfaces",
    "@Listeners": "prod/src/listeners",
    "@Models": "prod/src/database/models",
    "@Utils": "prod/src/utils"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "discord.js": "^12.5.1",
    "mathjs": "^8.1.0",
    "module-alias": "^2.2.2",
    "mongoose": "^5.10.18"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/expect": "^24.3.0",
    "@types/mathjs": "^6.0.9",
    "@types/mocha": "^8.2.0",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.14",
    "@types/sinon": "^9.0.9",
    "@types/sinon-chai": "^3.2.5",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.2",
    "sinon-chai": "^3.5.0",
    "ts-mock-imports": "^1.3.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
